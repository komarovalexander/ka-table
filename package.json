{
  "name": "ka-table",
  "version": "2.3.1",
  "license": "MIT",
  "repository": "github:komarovalexander/ka-table",
  "homepage": "https://komarovalexander.github.io/ka-table/#/command-column",
  "dependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/react": "16.9.17",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "react-scripts test --watchAll",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "pack": "gulp build && cd dist && npm pack && cd .."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "13.1.4",
    "@types/react-dom": "16.9.4",
    "@types/react-highlight": "^0.12.2",
    "@types/react-router-dom": "^5.1.3",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gulp": "^4.0.2",
    "gulp-file": "^0.4.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "jsonfile": "^5.0.0",
    "node-sass": "^4.13.0",
    "react-filter-control": "^2.1.0",
    "react-ga": "^2.7.0",
    "react-highlight": "^0.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "3.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/lib/Components/**/*.{ts,tsx}",
      "src/lib/Utils/*.ts"
    ]
  },
  "keywords": [
    "datagrid",
    "react",
    "table",
    "component",
    "widget"
  ]
}
